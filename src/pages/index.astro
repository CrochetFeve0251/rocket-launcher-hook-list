---
import getHooks from "../lib/hooks";
// Component Imports
import SearchList, {Hook} from "../components/SearchList";
import BaseLayout from '../layouts/BaseLayout.astro';
const hooks = await getHooks();
const names = hooks.hooks.map(hook => {return {
	    name: hook.name,
		type: hook.type,
		summery: hook.summery,
} as Hook;});
const title = hooks.title;
const description = hooks.description;
---
<BaseLayout title={title} description={description}>
    <SearchList list={names} client:visible/>
</BaseLayout>
